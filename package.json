{
  "name": "natures-delight-with-iles-tailwindcss",
  "version": "0.0.0",
  "homepage": "https://natures-delight-with-iles-tailwindcss.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/pinegrow/natures-delight-with-iles-tailwindcss.git"
  },
  "scripts": {
    "dev": "iles dev",
    "now": "npm run build && npm run preview",
    "lighthouse:dev": "npx unlighthouse --no-cache --site http://localhost:5173",
    "lighthouse": "npx unlighthouse --no-cache --site https://natures-delight-with-iles-tailwindcss.netlify.app",
    "build": "iles build",
    "preview": "iles preview --open --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "vue-devtools": "vue-devtools",
    "lint": "npm run lint:fix && npm run format",
    "lint:fix": "eslint --fix --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts ./ --ignore-path .gitignore",
    "format": "prettier --write \"**/*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts,html,css,md,mdx,json}\" !public --ignore-path .gitignore"
  },
  "engines": {
    "node": "^14.18 || >= 16.0.0"
  },
  "dependencies": {
    "@vueuse/core": "^10.0.2",
    "@vueuse/head": "^2.0.0",
    "defu": "^6.1.2",
    "pinia": "^2.1.6",
    "vee-validate": "^4.11.6"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.51",
    "@islands/excerpt": "^0.9.0",
    "@islands/feed": "^0.8.1",
    "@islands/headings": "^0.8.2",
    "@islands/prism": "^0.8.0",
    "@nuxt/eslint-config": "^0.1.1",
    "@pinegrow/iles-module": "^3.0.56",
    "@pinegrow/tailwindcss-plugin": "^3.0.20",
    "@preact/preset-vite": "^2.5.0",
    "@sveltejs/vite-plugin-svelte": "^2.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^20.5.0",
    "@unocss/preset-icons": "^0.57.2",
    "@vue/devtools": "^6.5.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "iles": "0.9.5",
    "npm-run-all": "^4.1.5",
    "ofetch": "^1.0.1",
    "postcss": "^8.4.22",
    "preact": "^10.15.1",
    "preact-render-to-string": "^6.1.0",
    "prettier": "^2.8.4",
    "rehype-external-links": "^2.1.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "solid-js": "^1.7.6",
    "svelte": "^4.0.0",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0",
    "unocss": "^0.57.2",
    "unplugin-auto-import": "^0.16.6",
    "vite-plugin-vue-devtools": "^0.0.22",
    "vue-tsc": "^1.2"
  },
  "web-types": "web-types.json",
  "pnpm": {
    "overrides": {
      "vue": "3.3",
      "vite": "4.4"
    }
  }
}
